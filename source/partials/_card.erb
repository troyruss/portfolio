<% data.projects.each do |project| %>
    <span><%= project.name %></span>
    <span><%= project.description %></span>
    <span><%= project.url %></span>
    <span><%= project.github %></span>
    <ul>
        <% project.tags.each do |tag| %>
            <li> <%= tag %> </li>
        <% end %>
    </ul>
 <% end %>

 <div class="card">
     <div class="card-content">
        <div class="card-tags">
            <span class="card-tag">Rails</span>
            <span class="card-tag">JavaScript</span>
            <span class="card-tag">SCSS</span>
            <span class="card-tag">PostgreSQL</span>
            <span class="card-tag">Otherthing</span>
        </div>
        <h2 class="card-name">Project Name</h2>
        <p class="card-description">Lorem ipsum dolor sit, amet consectetur adipisicing elit.</p>
        <div class="link-group">
            <div class="icons8-github"></div>
            <a href="#" class="button">Check it out</a>
        </div>
    </div>
 </div>